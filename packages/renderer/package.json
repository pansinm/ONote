  "version": "0.11.0",
  "type": "commonjs",
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "compile": "cross-env NODE_ENV=production npm run build && cross-env NODE_ENV=production electron-builder --config .electron-builder.config.js",
    "compile:win": "cross-env NODE_ENV=production npm run build && cross-env NODE_ENV=production electron-builder --config .electron-builder.config.js --win",
    "bdd": "chokidar \"features/**\" -c \"yarn  cucumber-js\"",
    "test:watch": "yarn jest --watch",
    "test": "jest --coverage=false && yarn build && yarn test:e2e",
    "test:e2e": "yarn cucumber-js",
    "watch": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192\" node scripts/watch.js",
    "lint": "eslint . --ext js,ts,tsx"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom"
    ],
    "moduleNameMapper": {
      "^/@/(.*)$": "<rootDir>/src/$1"
    },
    "modulePaths": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!react-markdown|mdast-util-definitions|monaco-editor)"
    ],
    "testEnvironment": "jsdom",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "extensionsToTreatAsEsm": [
      ".ts",
      ".tsx"
    ],
    "globals": {
      "ts-jest": {
        "useESM": true,
        "tsconfig": "./tsconfig.json"
      }
    }
  },
